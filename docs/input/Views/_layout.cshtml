<!doctype html>
<html lang="en">
<head>
	@Html.Partial("_MetaData.cshtml")
</head>
<body>

	<nav>
		<div class="container">
			<h1><a href="./"><img src="./img/ditto_light.png" alt="Ditto" /></a></h1>
			<div id="menu">
				<ul class="toplinks">
					<li><a href="https://github.com/leekelleher/umbraco-ditto/">Github</a></li>
					<li><a href="https://our.umbraco.org/projects/developer-tools/ditto/">Our Umbraco</a></li>
				</ul>
			</div>
			<a id="menu-toggle" href="#" class=" ">&#9776;</a> 
		</div>
	</nav>

	<header>
		<div class="container">
			<h2 class="docs-header">@Html.Raw(ViewData["Title"] ?? Model.String("title"))</h2>
		</div>
	</header>

	<section>

		<div class="container">

			@Html.Partial("_Nav.cshtml")

			<div class="docs-content">
				@RenderBody()
			</div>

		</div>

  	</section>

	<section class="vibrant centered">
		<div class="container">
			<h4><a rel="license" href="http://creativecommons.org/licenses/by-sa/4.0/deed.en_GB">All content is licensed under Creative Commons Attribution-ShareAlike 4.0 International License</a></h4>
		</div>
	</section>

	<footer>
		<div class="container">
			<p> Copyright  &copy; 2014-@DateTime.UtcNow.Year Umbrella Inc, Our Umbraco and <a href="https://github.com/leekelleher/umbraco-ditto/graphs/contributors" target="_blank">other contributors</a>. This website was generated by <a href="http://wyam.io">Wyam</a>.</p>
		</div>
	</footer>

	<script src="./js/jquery.min.js"></script> 
   
	<script type="text/javascript" src="./js/prettify.js"></script> 
	<script src="https://google-code-prettify.googlecode.com/svn/loader/run_prettify.js?lang=css&skin=sunburst"></script>
	<script src="./js/layout.js"></script>
	<script src="./js/jquery.scrollTo-1.4.3.1.js" type="text/javascript"></script>

</body>
</html>